
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"]</title>
    <base href="/">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- CUSTOMER -->
    <link rel="stylesheet" href="~/css/cus/bootstrap.min.css">
    <!-- owl.carousel.2.0.0-beta.2.4 css -->
    <link rel="stylesheet" href="~/css/cus/owl.carousel.min.css">
    <!-- font-awesome v4.6.3 css -->
    <link rel="stylesheet" href="~/css/cus/font-awesome.min.css">
    <!-- flaticon.css -->
    <link rel="stylesheet" href="~/css/cus/flaticon.css">
    <!-- jquery-ui.css -->
    <link rel="stylesheet" href="~/css/cus/jquery-ui.css">
    <!-- metisMenu.min.css -->
    <link rel="stylesheet" href="~/css/cus/metisMenu.min.css">
    <!-- slicknav.min.css -->
    <link rel="stylesheet" href="~/css/cus/slicknav.min.css">
    <!-- swiper.min.css -->
    <link rel="stylesheet" href="~/css/cus/swiper.min.css">
    <!-- style css -->
    <link rel="stylesheet" href="~/css/cus/styles.css">
    <!-- responsive css -->
    <link rel="stylesheet" href="~/css/cus/responsive.css">
    @RenderSection("Css", required: false)

    <style>
        .pointer {
            cursor: pointer;
        }
    </style>
</head>

<body class="text-center">
    @await Html.PartialAsync("_Header.cshtml")
    @RenderBody()
    @await Html.PartialAsync("_Footer.cshtml")
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="~/js/cus/bootstrap.min.js"></script>
    <!-- owl.carousel.2.0.0-beta.2.4 css -->
    <script src="~/js/cus/owl.carousel.min.js"></script>
    <!-- mouse_scroll.js -->
    <script src="~/js/cus/mouse_scroll.js"></script>
    <!-- scrollup.js -->
    <script src="~/js/cus/scrollup.js"></script>
    <!-- slicknav.js -->
    <script src="~/js/cus/slicknav.js"></script>
    <!-- jquery.zoom.min.js -->
    <script src="~/js/cus/jquery.zoom.min.js"></script>
    <!-- swiper.min.js -->
    <script src="~/js/cus/swiper.min.js"></script>
    <!-- metisMenu.min.js -->
    <script src="~/js/cus/metisMenu.min.js"></script>
    <!-- mailchimp.js -->
    <script src="~/js/cus/mailchimp.js"></script>
    <!-- jquery-ui.min.js -->
    <script src="~/js/cus/jquery-ui.min.js"></script>
    <!-- main js -->
    <script src="~/js/cus/scripts.js"></script>
    @RenderSection("Scripts", required: false)
    @await Component.InvokeAsync("NToastNotify")
</body>

</html>
